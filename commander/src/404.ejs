    <style>
        body {
            font-family: 'Lato', serif;
            background-color: #006ff1;
        }
        .wrapper {
            display: table;
            position: fixed;
            width: 100%;
            height: 100%;
        }
        .coming-soon {
            color: #fff;
            padding-bottom: 200px;
            display: table-cell;
            text-align: center;
            vertical-align: middle;
        }

        h1 {
            font-size: 38px;
            font-weight: 300;
            margin-bottom: 10px;
            margin-top: 0;
        }

        p {
            font-weight: 300;
            font-size: 18px;
        }
    </style>
</head>
<body>
    <div class="wrapper">
        <div class="coming-soon">
            <img src="${require('src/img/cobalt_logo_white.svg')}" style="width: 200px;" /><br />
            <h1>404 Not Found</h1>
            <p>
                The page you are looking for could not be found..
            </p>
        </div>
    </div>
</body>
</html>
